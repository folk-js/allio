{
  "$schema": "https://schema.tauri.app/config/2.5.1",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "entitlements": "app.entitlements",
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    }
  },
  "productName": "overlay",
  "mainBinaryName": "overlay",
  "version": "0.0.0",
  "identifier": "com.tauri.dev",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "overlay",
        "label": "main",
        "width": 800,
        "height": 600,
        "x": 0,
        "y": 0,
        "transparent": true,
        "alwaysOnTop": true,
        "resizable": false,
        "decorations": false,
        "useHttpsScheme": true,
        "skipTaskbar": true,
        "visible": false
      }
    ],
    "macOSPrivateApi": true
  }
}