{
  "name": "axio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "tauri dev",
    "build": "tsc && vite build",
    "check": "cargo check",
