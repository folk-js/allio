{
  "name": "axio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "tauri dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "typegen": "TS_RS_EXPORT_DIR=$PWD cargo test -p axio -p axio-ws export_bindings",
    "client:dev": "vite --config vite.client.config.ts",
    "client:build": "vite build --config vite.client.config.ts",
    "client:preview": "vite preview --config vite.client.config.ts"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.5.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}