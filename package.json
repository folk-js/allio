    "profile:leaks": "./scripts/check-leaks.sh",
    "profile:watch": "./scripts/watch-memory.sh"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.5.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}